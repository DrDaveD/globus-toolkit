install-gptexecshareSCRIPTS: $(gptexecshare_SCRIPTS)
	@$(NORMAL_INSTALL)
	test -z "$(gptexecsharedir)" || $(MKDIR_P) "$(DESTDIR)$(gptexecsharedir)"
	@list='$(gptexecshare_SCRIPTS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    echo " $(gptexecshareSCRIPT_INSTALL) '$$d$$p' '$(DESTDIR)$(gptexecsharedir)/$$f'"; \
	    $(gptexecshareSCRIPT_INSTALL) "$$d$$p" "$(DESTDIR)$(gptexecsharedir)/$$f"; \
	  else :; fi; \
	done

uninstall-gptexecshareSCRIPTS:
	@$(NORMAL_UNINSTALL)
	@list='$(gptexecshare_SCRIPTS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	  echo " rm -f '$(DESTDIR)$(gptexecsharedir)/$$f'"; \
	  rm -f "$(DESTDIR)$(gptexecsharedir)/$$f"; \
	done
install-libexecSCRIPTS: $(libexec_SCRIPTS)
	@$(NORMAL_INSTALL)
	test -z "$(libexecdir)" || $(MKDIR_P) "$(DESTDIR)$(libexecdir)"
	@list='$(libexec_SCRIPTS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    echo " $(libexecSCRIPT_INSTALL) '$$d$$p' '$(DESTDIR)$(libexecdir)/$$f'"; \
	    $(libexecSCRIPT_INSTALL) "$$d$$p" "$(DESTDIR)$(libexecdir)/$$f"; \
	  else :; fi; \
	done

uninstall-libexecSCRIPTS:
	@$(NORMAL_UNINSTALL)
