digraph d
{
    node [ fillcolor=violetred style=filled  ];
    client [shape="record" label="{Client | <0> Client API}"];

    gatekeeper [ label="Gatekeeper\n(root)" ];

    jobmanager [ label="{<0> Job Manager (user)|<1> Scheduler Specific Plugin}" 
                 shape=record];
    job [ label="Job Process(es)" ];

    client:0 -> gatekeeper [ label="Job Request" ];
    client:0 -> jobmanager:0 [ label="Job Cancel" ];

    jobmanager:0 -> client:0 [ label="State Change Callback" ];

    gatekeeper -> jobmanager:0 [ label="fork/su/exec" ];
    jobmanager:1 -> job [ label="fork/exec/wait\nspsubmit/spq\ncondor,lsf" ];
};
