<?xml version="1.0"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="ssl_utils">
	<Build>
		<Settings>
			<Platform
				Name="Win32"/>
			<Configuration
				Name="Debug|Win32"
				IntermediateDirectory="Build"
				OutputDirectory="Build"
				ConfigurationType="4"
				CharacterSet="2">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="0"
					IncludePaths="$(GLOBUS_LOCATION)\include;$(ProjFileName)\oldgaa"
					Defines="WIN32;_DEBUG;_CONSOLE"
					MinimalRebuild="TRUE"
					FullRuntimeChecks="TRUE"
					RuntimeLibrary="5"
					TreatWChar_tAsBuiltInType="TRUE"
					UsePrecompiledHeader="0"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="4"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLibrarianTool"
					OutputFile="$(OutDir)\globus_sslutils.lib"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
			</Configuration>
			<Configuration
				Name="Release|Win32"
				IntermediateDirectory="Release"
				OutputDirectory="Release"
				ConfigurationType="4"
				CharacterSet="2">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="2"
					InlineFunctionExpansion="1"
					FramePointerOmission="TRUE"
					Defines="WIN32;NDEBUG;_CONSOLE"
					StringPooling="TRUE"
					RuntimeLibrary="4"
					PackageFunctions="TRUE"
					UsePrecompiledHeader="0"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="3"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLibrarianTool"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
			</Configuration>
		</Settings>
	</Build>
	<Files>
		<Filter
			Name="Source Files"
			Filter="c;def;odl;idl;">
			<File
				RelativePath="..\library\scutils.c">
			</File>
			<File
				RelativePath="..\library\sslutils.c">
			</File>
			<File
				RelativePath="..\library\winglue.c">
			</File>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h">
			<File
				RelativePath="..\library\config.h">
			</File>
			<File
				RelativePath="..\library\oldgaa\oldgaa_utils.h">
			</File>
			<File
				RelativePath="..\library\scutils.h">
			</File>
			<File
				RelativePath="..\library\sslutils.h">
			</File>
			<File
				RelativePath="..\library\winglue.h">
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
